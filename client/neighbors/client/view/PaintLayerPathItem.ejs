<%- (selectedPath === i) ?
        (`<i class="fa fa-fw fa-paint-brush" aria-hidden="true"></i>`) :
        (`<i class="fa fa-fw fa-bars muted" aria-hidden="true"></i>`) %>
<span id="toggle-path-visible-<%= i %>"><%- (path.visible) ?
            (`<i class="fa fa-fw fa-eye" aria-hidden="true"></i>`) :
            (`<i class="fa fa-fw fa-eye-slash muted" aria-hidden="true"></i>`) %></span>
<%= layer.name?(layer.name+" "):"" %><%= path.name||`Path ${i+1}` %><span class="right">
    <span class="btn slim disabled" id="toggle-path-node-<%= i %>"><i
                class="fa-solid fa-fw fa-ruler" style="padding-left: 0;"></i><%= path.strokes.length %></span> <span id="toggle-path-delete-<%= i %>">
     <i class="far fa-fw fa-trash-alt" aria-hidden="true"></i>

    </span></span>
