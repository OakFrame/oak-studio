<div id="map-page" style="position: fixed;width: 100%;min-height:100%;height: 100vh;">
    <div class="" id="canvas-surface-error-log" style="z-index: 1;width: 100%;position:absolute;"></div>
    <canvas class="fixed-canvas" id="canvas-surface-gl"></canvas>
    <canvas class="fixed-canvas" id="canvas-surface-2d" style="pointer-events: none;"></canvas>
    <div id="context-menu-location"></div>
</div>
<div id="work-window" style="position: relative;">
    <div class="container">
        <div id="bar-map-action" class="bar action"></div>
    </div>

    <div class="container">
        <div id="examine-window">
        </div>
        <div id="depthTexture"></div>
        <div id="SSAO"></div>
    </div>
</div>

<div class="bar bottom safari-only" id="map-bar-bottom">

    <div class="container footer-window map" style="display:none;" id="footer-window-map">

        <div class="bar map-tools bar-grid-fit-items">

            <div class="btn game-button-label" id="spell-label" data-uuid="label"><i
                        class="fa-solid fa-fw fa-map"></i><span>tools</span></div>

            <div class="btn game-button prefab-button" id="map-place-tree" data-uuid="prefab-tree-pine-00"><i
                        class="fa-solid fa-fw fa-tree"></i><span>pine00</span></div>

        </div>

        <div class="bar map-tools bar-grid-fit-items">

            <div class="btn game-button-label" id="spell-label" data-uuid="label"><i
                        class="fa-solid fa-fw fa-map"></i><span>hashMap</span></div>

            <div class="btn game-button" id="map-control-regenerate"><i
                        class="fa-solid fa-fw fa-sync"></i><span>regenerate</span></div>

            <div class="btn game-button" id="map-control-blur"><i
                        class="fa-solid fa-fw fa-bezier-curve"></i><span>blur</span></div>

            <div class="btn game-button" id="map-control-biome"><i
                        class="fa-solid fa-fw fa-bezier-curve"></i><span>biome</span></div>

            <div class="btn game-button" id="map-control-techtonics"><i
                        class="fa-solid fa-fw fa-layer-group"></i><span>techtonics</span></div>

            <div class="btn game-button" id="map-control-erode"><i
                        class="fa-solid fa-fw fa-mountain"></i><span>erode</span></div>

            <div class="btn game-button" id="map-control-simulate"><i
                        class="fa-solid fa-fw fa-volcano"></i><span>simulate</span></div>

        </div>

        <div class="bar map-tools bar-grid-fit-items">

            <div class="btn game-button-label" ><i
                        class="fa-solid fa-fw fa-gear"></i><span>settings</span></div>

            <div class="btn game-button setting-button" data-uuid="USE_CLOUDS"><i
                        class="fa-solid fa-fw fa-cloud"></i><span>clouds</span></div>

            <div class="btn game-button setting-button" data-uuid="USE_FOG"><i
                        class="fa-solid fa-fw fa-smog"></i><span>fog</span></div>

            <div class="btn game-button setting-button" data-uuid="USE_FOG_VOLUME"><i
                        class="fa-solid fa-fw fa-layer-group"></i><span>v. fog</span></div>

            <div class="btn game-button setting-button" data-uuid="USE_FOG_WARBLE"><i
                        class="fa-solid fa-fw fa-water"></i><span>f. warble</span></div>

            <div class="btn game-button setting-button" data-uuid="USE_SHADOWS"><i
                        class="fa-solid fa-fw fa-sun"></i><span>shadow</span></div>

            <div class="btn game-button setting-button" data-uuid="USE_PCF"><i
                        class="fa-solid fa-fw fa-gear"></i><span>PCF</span></div>

            <div class="btn game-button setting-button" data-uuid="USE_FBM"><i
                        class="fa-solid fa-fw fa-gear"></i><span>FBM</span></div>

            <div class="btn game-button setting-button" data-uuid="USE_CARTOON"><i
                        class="fa-solid fa-fw fa-gear"></i><span>toon</span></div>


        </div>
    </div>

    <div class="container footer-window stats" style="display:none;" id="footer-window-stats">
        <div id="footer-window-stats-inner" class="bar-grid-fit-items stack"></div>
    </div>


    <div class="container footer-window spells" style="display:none;" id="footer-window-spells">
        <div class="bar-grid-fit-items">
            <div class="btn spell-button-label" id="spell-label" data-uuid="label"><i
                        class="fa-solid fa-fw fa-certificate"></i><span>spells</span></div>
            <div class="btn spell-button" id="spell-focus" data-uuid="focus"><i
                        class="fa-solid fa-fw fa-certificate"></i><span>focus</span></div>
            <div class="btn spell-button" id="spell-heal" data-uuid="heal"><i
                        class="fa-solid fa-fw fa-star-of-life"></i><span>heal</span></div>
            <div class="btn spell-button" id="spell-teleport" data-uuid="teleport"><i
                        class="fa-solid fa-fw fa-person-arrow-down-to-line"></i><span>teleport</span></div>
            <div class="btn spell-button" id="spell-bolt" data-uuid="bolt"><i
                        class="fa-solid fa-fw fa-bolt"></i><span>bolt</span></div>
            <div class="btn spell-button" id="spell-blast" data-uuid="blast"><i
                        class="fa-solid fa-fw fa-burst"></i><span>blast</span>
            </div>
            <div class="btn spell-button" id="spell-cure" data-uuid="cure"><i
                        class="fa-solid fa-fw fa-heart"></i><span>cure</span></div>
            <div class="btn spell-button" id="spell-bloom" data-uuid="bloom"><i
                        class="fa-solid fa-fw fa-plant-wilt"></i><span>bloom</span></div>
            <div class="btn spell-button" id="spell-possess" data-uuid="possess"><i
                        class="fa-solid fa-fw fa-brain"></i><span>possess</span></div>
        </div>
    </div>

    <div class="container bar">
        <div class="bar-grid-fit-items">

        <span class="btn game-button" id="toggle-log"><i class="fa-solid fa-fw fa-list-ol"></i><span
                    class="hide-mobile"> Toggle</span></span>
            <span class="btn game-button" id="toggle-map"><i class="fa-solid fa-fw fa-map"></i><span
                        class="hide-mobile"> Map</span></span>
            <span class="btn game-button" id="toggle-stats"><i class="fa-solid fa-fw fa-shield-halved"></i><span
                        class="hide-mobile"> Stats</span></span>
            <span class="btn game-button" id="toggle-inventory"><i class="fa-solid fa-fw fa-toolbox"></i><span
                        class="hide-mobile"> Inventory</span></span>
            <div class="btn game-button" id="toggle-spells"><i class="fa-solid fa-fw fa-skull-crossbones"></i><span
                        class="hide-mobile"> Spells</span></div>
            <span class="btn game-button" id="toggle-prayer"><i class="fa-solid fa-fw fa-star"></i><span
                        class="hide-mobile"> Prayer</span></span>
            <span class="btn game-button" id="toggle-equipment"><i class="fa-solid fa-fw fa-shield-halved"></i><span
                        class="hide-mobile"> Equipment</span></span>

        </div>
